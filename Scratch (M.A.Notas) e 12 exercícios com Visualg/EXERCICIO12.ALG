Algoritmo "Exercicio12"

Var
   ano_nasc, diferenca_ano: inteiro
   dias_totais: inteiro
   
Inicio
      Escreva("Digite o ano do seu nascimento: ")
      Leia(ano_nasc)
      diferenca_ano <- 2020 - ano_nasc
      dias_totais <- diferenca_ano*360
      Escreva("Você tem", dias_totais, " dias vividos.")
Fimalgoritmo