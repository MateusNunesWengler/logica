Algoritmo "SomaPAR85ate907"

Var
   number, CONT: inteiro
   soma: real
Inicio
 number <- 84
 enquanto number < 906 faca
      CONT <- CONT + 1
      number <- number + 2
      escreval(number)
 fimenquanto
 soma <- (86 + 906)*CONT/2 //fórmula da soma de "n" termos de uma PA
 Escreva("A soma é: ", soma)
Fimalgoritmo