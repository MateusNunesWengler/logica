Algoritmo "Exercicio6-FahParaCel"

Var
   F, c: real
Inicio
   escreval("<==================================>")
   escreval("CONVERSÃO DE FAHRENHEIT PARA CELSIUS")
   escreval("<==================================>")
   escreva("Digite o valor da temperatura em graus Fahrenheit: ")
   leia(F)
   c <- (F - 32)*5/9
   escreva("O valor em grau celsius é:", c)
Fimalgoritmo