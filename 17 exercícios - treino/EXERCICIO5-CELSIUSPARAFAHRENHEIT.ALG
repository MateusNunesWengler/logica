Algoritmo "Exercicio5-CelsiusparaFahrenheit"

Var
   c, F: real
Inicio
  escreval("<====================================>")
  escreval(" CONVERSÃO DE CELSIUS PARA FAHRENHEIT")
  escreval("<====================================>")
  escreva("Digite a temperatura graus celsius: ")
  leia(c)
  F <- (9*c/5) + 32
  escreva("O valor em graus fahrenheit é:", F)
Fimalgoritmo